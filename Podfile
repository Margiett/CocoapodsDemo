# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'CocoapodsDemo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CocoapodsDemo
  
  #alamofire networkking library
  pod 'Alamofire'# not specifying the version number will install the most recent version of the library
  # Snapkit is a Autolayout third party library
  pod 'SnapKit'
  #kingfisher is a photo procession library
pod 'Kingfisher'

  target 'CocoapodsDemoTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
